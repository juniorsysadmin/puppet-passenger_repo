{
  "name": "juniorsysadmin-passenger_repo",
  "version": "0.3.0",
  "author": "juniorsysadmin",
  "summary": "Module for installing the Phusion Passenger OSS/Enterprise APT repository",
  "license": "Apache-2.0",
  "source": "https://github.com/juniorsysadmin/puppet-passenger_repo.git",
  "project_page": "https://github.com/juniorsysadmin/puppet-passenger_repo",
  "issues_url": "https://github.com/juniorsysadmin/puppet-passenger_repo/issues",
  "tags": ["passenger"],
  "operatingsystem_support": [
